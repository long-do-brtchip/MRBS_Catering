{
  "name": "panlhub",
  "version": "1.0.0",
  "description": "PanL Hub",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && npm run web && npm run watch",
    "watch": "nodemon -e ts -w ./src -x npm run serve",
    "serve": "ts-node --inspect src/index.ts",
    "build": "tsc --skipLibCheck && npm run tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "NODE_ENV=test nyc mocha test/**/*.ts",
    "web": "[ ! -d web/src ] && git submodule update --init --recursive; cd web; [ ! -d node_modules ] && npm install; npx ng build && cd -",
    "prepush": "npm run build"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist",
      "coverage"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "author": "BRTCHIP",
  "license": "SEE LICENSE IN <LICENSE>",
  "dependencies": {
    "class-validator": "^0.7.3",
    "ews-javascript-api": "^0.9.1",
    "ews-javascript-api-auth": "^1.2.0",
    "express": "^4.16.2",
    "int64-buffer": "^0.1.9",
    "ioredis": "^3.2.1",
    "moment": "^2.19.1",
    "ref": "^1.3.5",
    "ref-array": "^1.2.0",
    "ref-struct": "^1.1.0",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^3.1.13",
    "typeorm": "^0.1.6",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.39",
    "@types/ioredis": "0.0.25",
    "@types/mocha": "^2.2.43",
    "@types/moment-timezone": "^0.5.2",
    "@types/node": "^8.0.50",
    "@types/ref": "0.0.28",
    "@types/rewire": "^2.5.28",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "rewire": "^2.5.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "uuid": "^3.1.0"
  }
}
