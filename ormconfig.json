{
   "name": "default",
   "type": "sqlite",
   "database": "hub.db",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/entity/hub/*.ts"
   ],
   "migrations": [
      "src/migration/hub/*.ts"
   ],
   "subscribers": [
      "src/subscriber/hub/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity/hub",
      "migrationsDir": "src/migration/hub",
      "subscribersDir": "src/subscriber/hub"
   }
}